    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "styled-components": "^5.1.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@types/jest": "^26.0.15",
    "@types/next": "^9.0.0",
    "@types/react": "^16.9.56",
