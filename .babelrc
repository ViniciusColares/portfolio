{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "useBuiltIns": "usage"
        }
      }
    ]
  ],
  "plugins": [
    ["inline-react-svg"],
    ["@babel/syntax-dynamic-import"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@modules": "./node_modules",
          "@components": "./components",
          "@templates": "./templates",
          "@styles": "./styles",
          "@public": "./public",
          "@pages": "./pages"
        }
      }
    ],
    [
      "lodash",
      {
        "id": ["lodash"]
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": false
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [["@babel/preset-env", { "modules": false }], "next/babel"]
    }
  }
}
